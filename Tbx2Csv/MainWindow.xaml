<Window x:Class="Tbx2Csv.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:Tbx2Csv.ViewModel"
        xmlns:v="clr-namespace:Tbx2Csv.View"
        Title="{Binding Path=DisplayName}" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition Height="4*"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="2" FontSize="10" Text="{Binding Path=Tbx2CsvVersion}"></TextBlock>
    </Grid>
</Window>
